<div>
    <mail>
        <ol>
            {{#with heading}}
                <br>
                <br>
                <div class="card">
                    <div class="card-body">
                        <div class="card">
                            <div class="card-body">
                                {{#with main}}
                                    <h1>{{title}}</h1>
                                    <br>
                                    <b>{{text}}</b>
                                    <br>
                                    <br>
                                    {{#with button_text}}
                                        <button class="btn btn-primary">{{solid}}</button>
                                        <br>
                                        <br>
                                        <button class="btn btn-outline-primary">{{outline}}</button>
                                    {{/with}}
                                {{/with}}
                            </div>
                        </div>
                        <br>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with second}}
                                    <h2>{{title}}</h2>
                                    <br>
                                    <b>{{text}}</b>
                                    <br>
                                    <br>
                                    {{#with button_text}}
                                        <button class="btn btn-secondary">{{solid}}</button>
                                        <br>
                                        <br>
                                        <button class="btn btn-outline-secondary">{{outline}}</button>
                                    {{/with}}
                                {{/with}}
                            </div>
                        </div>
                        <br>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with subblock}}
                                    <h3>{{title}}</h3>
                                    <br>
                                    <b>{{text}}</b>
                                    <br>
                                    <br>
                                    {{#with button_text}}
                                        <button class="btn btn-dark">{{solid}}</button>
                                        <br>
                                        <br>
                                        <button class="btn btn-outline-dark">{{outline}}</button>
                                    {{/with}}
                                {{/with}}
                            </div>
                        </div>
                    </div>
                </div>
            {{/with}}
        </ol>
        <br>
        <br>
        <br>
        <div class="card">
            <div class="card-body">
                <ol>
                    {{#with nested_display}}
                    <div class="display-1"><strong>{{title}}</strong></div>
                    <br>
                    <br>
                        <div class="card">
                            <div class="card-body">
                                    {{#with children_display}}
                                        <div class="display-4"><strong>{{title}}</strong></div>
                                        <br>
                                        <p>{{text}}</p>
                                        <br>
                                        {{#with button}}
                                            <button class="btn btn-outline-warning">{{text}}</button>
                                        {{/with}}
                                    {{/with}}
                            </div>
                        </div>
                        <br>
                        <br>
                        <div class="display-6"><strong>{{header_text}}</strong></div>
                        <br>
                        <br>
                        {{#with button}}
                            <button class="btn btn-info">{{text}}</button>
                        {{/with}}
                    {{/with}}
                </ol>
            </div>
        </div>
        <br>
        <br>
        <div class="card">
            <div class="card-body">
                <ol>
                    {{#with paragraph}}
                        <div class="text-uppercase display-6">{{title}}</div>
                        <br>
                        <br>
                        <div class="text-lowercase display-7">{{title}}</div>
                        <br>
                        <br>
                        {{#with button}}
                            <a href="{{url}}" class="btn btn-link">{{text}}</a>
                        {{/with}}
                        <br>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                <ul>
                                    {{#each list_name}}
                                        <li>
                                            <a href="{{url}}" class="btn btn-link">{{text}}</a>
                                        </li>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    {{/with}}
                </ol>
            </div>
        </div>
    </mail>
</div>